<h1>Raffle</h1>
<div ng-controller="RaffleCtrl as raffle">
  <form>
    <input type="text" ng-model="name"><br><br>
    <input type="submit">&nbsp;&nbsp;Add</input>
    <p>{{name}}</p>
    <ul>
      <li ng-repeat="entry in raffle.entries">
        {{entry.name}}
      </li>
    </ul>
  </form>
</div>